OBJS = GuillotineBinPack.o  main.o  MaxRectsBinPack.o  Rect.o  WorkDispatcher.o
CXX = g++
CPPFLAGS = -O3 -g -flto -Wall

LDFLAGS = -fwhole-program -pthread -lm

all: $(OBJS)
	$(CXX) $(CPPFLAGS) $(LDFLAGS) $(OBJS) -o rectpacker

clean:
	rm -rf $(OBJS)

test: all
	./rectpacker 0 0
	./rectpacker 1 1
	./rectpacker 1 1 2 2
	./rectpacker 1 1 1 1 1 1 1 1
	./rectpacker 2 1 2 1
	./rectpacker 20 36 25 36 28 35 26 36 20 35 24 36 27 36 26 36 20 36 25 36 28 35 26 36 20 35 24 37 27 36 26 36 21 37 25 37 28 35 26 37 20 36 25 37 27 37 26 37 21 37 25 37 28 36 26 38 20 37 25 38 28 37 27 37 21 37 25 38 28 36 26 38 21 37 25 38 28 37 27 38 21 37 25 37 28 36 26 38 20 37 25 38 28 37 27 37 20 36 25 37 28 35 26 37 20 36 25 37 27 37 26 37 20 36 25 36 28 35 26 36 20 35 24 37 27 36 26 36 20 36 25 36 28 35 26 36 20 35 24 36 27 36 26 36 21 37 26 37 30 36 28 38 20 37 25 38 30 37 27 37 21 40 27 40 33 37 30 39 21 39 26 39 33 38 31 39 21 40 27 39 32 37 29 39 21 40 26 40 34 38 30 39 21 38 26 38 33 36 29 40 20 40 27 41 35 38 32 37 21 37 26 37 31 36 28 39 20 39 26 40 32 37 30 37 20 36 25 36 28 35 26 37 20 36 25 37 29 36 27 37 20 36 25 36 28 35 26 36 20 35 24 36 28 36 27 36 21 38 26 38 33 36 29 40 20 40 27 41 35 38 32 37 21 37 26 37 31 36 28 39 20 39 26 40 32 37 30 37 20 36 25 36 28 35 26 37 20 36 25 37 29 36 27 37 20 36 25 36 28 35 26 36 20 35 24 36 28 36 27 36 23 39 26 38 31 41 28 45 20 46 27 46 33 43 31 40 25 42 28 41 35 44 32 50 22 51 31 51 41 47 37 43 26 43 30 42 41 45 36 51 25 52 35 53 48 49 43 44 33 37 30 39 45 43 42 52 32 59 38 56 54 46 48 39 37 28 30 28 48 27 45 34 36 42 35 38 52 30 46 21 40 30 38 29 51 22 46 21 40 29 38 28 51 25 47 23 43 32 43 29 57 24 50 23 43 29 42 27 57 25 49 25 44 32 45 30 59 24 52 24 44 31 45 27 58 26 51 26 23 39 26 38 31 41 28 45 20 46 27 46 33 43 31 40 25 42 28 41 35 44 32 50 22 51 31 51 41 47 37 43 26 43 30 42 41 45 36 51 25 52 35 53 48 49 43 44 33 37 30 39 45 43 42 52 32 59 38 56 54 46 48 39 37 28 30 28 48 27 45 34 36 42 35 38 52 30 46 21 40 30 38 29 51 22 46 21 40 29 38 28 51 25 47 23 43 32 43 29 57 24 50 23 43 29 42 27 57 25 49 25 44 32 45 30 59 24 52 24 44 31 45 27 58 26 51 26
